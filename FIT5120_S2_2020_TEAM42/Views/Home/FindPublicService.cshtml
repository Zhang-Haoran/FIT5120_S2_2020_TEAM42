<div id='map'></div>


<script>
    mapboxgl.accessToken = 'pk.eyJ1IjoiemhyMTk5NDEyMSIsImEiOiJjazB6NmozaGwwbHFzM2JyeGs5MHV4eW9iIn0.2kK8cbqWh3i87zRbRDIt4w';
    var map = new mapboxgl.Map({
        container: 'map',
        style: 'mapbox://styles/mapbox/streets-v11',
        center: [-79.4512, 43.6568],
        zoom: 13
    });

    map.addControl(new MapboxDirections({
        accessToken: mapboxgl.accessToken
    }), 'top-left');
</script>

