<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">
    <title>AyeMate</title>
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
    <script>
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <!--Vendor css-->
    @Styles.Render("~/Vendor/css/bootstrap.css")
    @Styles.Render("~/Vendor/css/style.css")
    @Styles.Render("~/Vendor/css/font-awesome.min.css")
    <link href="//fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
</head>
<body>

    <!-- inner banner -->
    <section class="inner-banner-w3ls d-flex flex-column justify-content-center align-items-center">
        <!-- header -->
        <header>
            <nav class="navbar navbar-expand-lg navbar-light bg-gradient-secondary">
                <h1>
                    <a class="navbar-brand" href="~/Home/Index">
                        Aye Mate
                    </a>
                </h1>
                <button class="navbar-toggler ml-md-auto" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-lg-auto text-center">
                        <li class="nav-item  mr-lg-3 mt-lg-0 mt-3">
                            <a class="nav-link" href="~/Home/Index">
                                Home
                            </a>
                        </li>


                        <li class="nav-item  mr-lg-3 mt-lg-0 mt-3">
                            <a class="nav-link" href="~/Home/KnowAnxiety">
                                Know Anxiety
                            </a>
                        </li>
                        <li class="nav-item  mr-lg-3 mt-lg-0 mt-3">
                            <a class="nav-link" href="~/Home/ImproveWellbeing">
                                Improve Wellbeing
                            </a>
                        </li>
                        <li class="nav-item dropdown mr-lg-3 mt-lg-0 mt-3">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                               aria-haspopup="true" aria-expanded="false">
                               Maintain Good habits
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <a class="dropdown-item" href="~/Home/Diet">Diet</a>
                                <a class="dropdown-item" href="~/Home/Sleep">Sleep</a>
                                <a class="dropdown-item" href="~/Home/Exercise">Exercise</a>
                                @*<div class="dropdown-divider"></div>
                <a class="dropdown-item scroll" href="#explore">explore</a>*@
                            </div>
                        </li>
                        <li class="nav-item  mr-lg-3 mt-lg-0 mt-3">
                            <a class="nav-link" href="~/Home/About">
                                About
                            </a>
                        </li>


                    </ul>
                </div>

            </nav>
        </header>
        <!-- //header -->
    </section>
    <!-- //inner banner -->
    <!-- breadcrumbs -->
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb d-flex justify-content-center bg-theme">
            <li class="breadcrumb-item">
                <a href="~/Home/Index">Home</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">@ViewContext.RouteData.Values["action"].ToString()</li>
        </ol>
    </nav>
    <!-- //breadcrumbs -->
    <!-- about-->
    <section class="single_grid_w3_main align-w3" id="about">
        <div class="container">
            <div class="wthree_pvt_title text-center">

                @RenderBody()
            </div>

        </div>
    </section>
    <!-- //about -->
    <!-- footer -->
    <footer class="footer py-md-5 pt-md-3 pb-sm-5">
        <div class="container-fluid">
            <div class="row p-sm-4 px-3 py-5">
                <div class="col-lg-4 col-md-6 footer-top mt-md-0 mt-sm-5">
                    <h2>
                        <a class="navbar-brand" href="~/Home/Index">
                            Aye Mate
                        </a>
                    </h2>
                    <p class="my-3 text-white">
                        AYE MATE! stands for<br />
                        M - Monitor your wellbeing!<br />
                        A - Ask for help!<br />
                        T - Take a step!<br />
                        E - Enhance your life!<br />
                    </p>
                    
                </div>
                <div class="col-lg-2  col-md-6 mt-md-0 mt-5">
                    <div class="footerv2-w3ls">
                        <h3 class="mb-3 w3f_title">Navigation</h3>
                        <hr>
                        <ul class="list-w3pvtits">
                            <li>
                                <a href="~/Home/Index">
                                    Home
                                </a>
                            </li>
                            <li>
                                <a href="~/Home/Stressbuster">
                                    Stressbuster
                                </a>
                            </li>
                            <li>
                                <a href="~/Home/About">
                                    About
                                </a>
                            </li>

                        </ul>
                    </div>
                </div>


            </div>
        </div>
        <!-- //footer bottom -->
    </footer>
    <!-- //footer -->
    <!-- copyright -->
    <div class="cpy-right text-center bg-theme">
        <p class="text-white">
            ©2020 AyeMate! All rights reserved | Design by
            <a href="http://w3layouts.com"> W3layouts.</a>
        </p>
    </div>
    <!-- //copyright -->
    <!--Vendor js-->
    @Scripts.Render("~/Vendor/js/jquery-2.2.3.min.js")
    @Scripts.Render("~/Vendor/js/responsiveslides.min.js")
    <script>
        // You can also use "$(window).load(function() {"
        $(function () {
            // Slideshow 4
            $(".slider3").responsiveSlides({
                auto: true,
                pager: true,
                nav: false,
                speed: 500,
                namespace: "callbacks",
                before: function () {
                    $('.events').append("<li>before event fired.</li>");
                },
                after: function () {
                    $('.events').append("<li>after event fired.</li>");
                }
            });

        });
    </script>
    @Scripts.Render("~/Vendor/js/move-top.js")
    @Scripts.Render("~/Vendor/js/easing.js")
    @Scripts.Render("~/Paint/js/bootstrap.min.js")

    <script>
        jQuery(document).ready(function ($) {
            $(".scroll").click(function (event) {
                event.preventDefault();

                $('html,body').animate({
                    scrollTop: $(this.hash).offset().top
                }, 1000);
            });
        });
    </script>
    <script>
        $(document).ready(function () {

            $().UItoTop({
                easingType: 'easeOutQuart'
            });

        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
